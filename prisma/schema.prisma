datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Note {
  id    String @id @default(cuid())

  title String
  body  String

  xpos  Int @default(0)
  ypos  Int @default(0)

  createdAt DateTime @default(now())
}
